@{}
    @model IEnumerable<EcdlBooking.ViewModel.Admin_Exam_Create>;


    <div class="container">


@if(@Model != null)
{


    <table class="table table-striped table-hover">
        <thead>
            <tr>
                <th scope="col">Data</th>
                <th scope="col">Ora Inizio</th>
                <th scope="col">Luogo</th>
                <th scope="col">Esaminatore</th>
                <th scope="col">  </th>

        </thead>
        <tbody>
            <!--     sperimentazione     -->
            @foreach (var i in @Model)
            {
                <tr>
                    <th scope="col">@i.Data.ToString("gg M yy")</th>
                    <th scope="col">@i.Data.ToString("hh:mm")</th>
                    <th>
                    <th scope="col>@i.</th>
                    <th scope="col">Esaminatore</th>
                    <th scope="col">
                        <a type="button" class="btn btn-primary" asp-route="@i.id">Cancella</a>
                        <a type="button" class="btn btn-primary" asp-route="@i.id">Modifica</a>
                        <a type="button" class="btn btn-primary" asp-route="@i.id">Visualizza</a>

<th>
                </tr>

            }


        </tbody>
    </table>








}

</div>